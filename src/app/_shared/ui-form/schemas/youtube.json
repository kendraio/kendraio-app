{
      "title": "A registration form",
      "description": "A simple form example.",
      "type": "object",
      "required": [
        "firstName",
        "isdn",
        "claim"
      ],
      "properties": {

        "artist": {
          "title": "Artist",
          "description": "Please select your Artist",
          "type": "string",
          "enum": ["David Boxie",
            "The Clash",
            "Sex Pistols",
            "The Fall"]
        },
        "firstName": {
          "type": "string",
          "title": "Record Label",
        },
        "isdn": {
          "type": "string",
          "title": "ISDN",
        },
        "country": {
          "type": "typeahead",
          "title": "Region",
          "items": {
            "type": "string",
            "default": "bazinga",
          },
        },
        "age": {
          "type": "number",
          "title": "View Count",
        },
        "simpleDate": {
          "title": "Release Date",
          "type": "datepicker",
        },
        "bio": {
          "type": "textarea",
          "title": "Bio",
        },
        "password": {
          "type": "string",
          "title": "Password",
          "minLength": 3,
        },
        "telephone": {
          "type": "radio",
          "title": "Genre",
          "enum": [
            "Punk",
            "Reggea",
            "Soul",
            "Jazz",
          ],
        },
        "pay": {
          "type": "money",
          "title": "Income",
          "minLength": 10,
          "description": "(Less is more)",
        },
        "claim": {
          "type": "percentage",
          "title": "Claim Share",
        },


      },
    }
